  \n     \n\n          /*setGlobalState({\n            profile: ...\n          })\n          */\n    //  alert(\"loggedIn status changed!\")\n    },\n    [ globalState.loggedIn, globalState.profile ]\n  )\n\n  return (\n    <AppContext.Provider value={[globalState, setGlobalState]}>\n      <BrowserRouter>\n          <Switch>\n              <LayoutRoute path=\"/\" exact={true} component={HomeScreen} />\n              <LayoutRoute path=\"/about\" exact={true} component={AboutScreen} />\n              <LayoutRoute path=\"/register\" exact={true} component={RegistrationScreen} />\n              <LayoutRoute path=\"/login\" exact={true} component={LoginScreen} />\n              <PrivateRoute path = \"/profile\" exact = {true} component = {ProfileScreen} />\n          </Switch>\n      </BrowserRouter>\n    </AppContext.Provider>\n  )\n}\n\nexport default App;","/Users/i340775/Documents/dev/frontend/src/reportWebVitals.js",[],["76","77"],"/Users/i340775/Documents/dev/frontend/src/NavBar.js",["78","79"],"import React from 'react'\n\nfunction NavBar(props){\n    return(\n    <nav className={\"navbar navbar-dark bg-dark\" + \" \" + props.className}>\n    <div className=\"container-fluid\">\n      <a className=\"navbar-brand\">Navbar</a>\n      <div className = \"d-flex flex-grow-1\">\n      {props.children}\n      </div>\n      {props.extraComponent}\n    \n    </div>\n  </nav>\n    );\n  }\n\n  export default NavBar;","/Users/i340775/Documents/dev/frontend/src/Button.js",[],"/Users/i340775/Documents/dev/frontend/src/Jumbotron.js",["80"],"import React from 'react';\n\n\n\nfunction Jumbotron(props){\n    \n    \n    return(\n      <div class=\"jumbotron\">\n    <h1 class=\"display-4\">{props.title}</h1>\n    <p class=\"lead\">{props.lead}</p>\n    <hr class=\"my-4\" />\n    <p>{props.description}</p>\n    <a class=\"btn btn-primary btn-lg\" href=\"#\" role=\"button\">{props.buttonLabel}</a>\n  </div>\n    );\n  }\n\n  export default Jumbotron;","/Users/i340775/Documents/dev/fr